@media screen and (min-width: 501px) {

	#<%= modulePart %>-frm {
		display: grid;
		grid-template-columns: repeat(6, 1fr);
		grid-auto-rows: auto;
		gap: 0;
		width: 850px;
	}

	#<%= modulePart %>-frm > div {
		padding: 5px;
	}

	<% fields.forEach(field => { %>
	#<%= field.elementId %>-container {
		grid-row: <%= field.dposrow %><%= field.dposrowspan %>;
		grid-column: <%= field.dposcol %><%= field.dposcolspan %>;
		<%= field.dposstyle %>
	}
	<% }) %>	


}